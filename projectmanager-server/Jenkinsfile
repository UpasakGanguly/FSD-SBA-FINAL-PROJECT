pipeline {
    agent any
    tools {
        maven 'maven'
        jdk 'JAVA-jdk'
    }
    stages {
        stage ('Build') {
            steps {
                    bat label: '', script: 'bat \'mvn install\''
            }
        }
        stage ('Docker-Build') {
            steps {
                    bat label: '', script: 'bat \'cd projectmanager-server' && mvn package docker:build'
            }
        }
    }
}
